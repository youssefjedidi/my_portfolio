{
    "name": "youssef-jedidi-portfolio",
    "version": "1.0.0",
    "description": "Youssef Jedidi - Computer Engineering Portfolio with AI/ML projects and full-stack development showcase",
    "author": "Youssef Jedidi",
    "private": true,
    "homepage": "https://youssefjedidi.vercel.app",
    "repository": {
        "type": "git",
        "url": "https://github.com/youssefjedidi/my_portfolio.git"
    },
    "keywords": [
        "portfolio",
        "react",
        "computer-engineering",
        "ai-ml",
        "full-stack",
        "web-development",
        "responsive",
        "pwa"
    ],
    "dependencies": {
        "bootstrap": "^5.2.3",
        "emailjs-com": "^3.2.0",
        "framer-motion": "^11.9.0",
        "react": "^18.2.0",
        "react-bootstrap": "^2.7.0",
        "react-dom": "^18.2.0",
        "react-helmet-async": "^1.0.7",
        "react-icons": "^4.1.0",
        "react-router-dom": "^6.6.2",
        "react-scripts": "^5.0.1",
        "react-transition-group": "^4.4.2",
        "typewriter-effect": "^2.18.2",
        "web-vitals": "^3.1.1"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "build:analyze": "npm run build -- --stats && npx webpack-bundle-analyzer build/static/js/*.js",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "predeploy": "yarn build && cp build/index.html build/404.html",
        "deploy": "gh-pages -d build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ],
        "globals": {
            "self": "readonly"
        },
        "overrides": [
            {
                "files": ["public/sw.js", "build/sw.js"],
                "env": {
                    "serviceworker": true
                },
                "globals": {
                    "self": "readonly",
                    "caches": "readonly",
                    "fetch": "readonly",
                    "clients": "readonly"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}
